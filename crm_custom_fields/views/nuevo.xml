<record id="custom_logo_in_navbar" model="ir.ui.view">
    <field name="name">Custom Logo in Navbar</field>
    <field name="mode">extension</field>
    <field name="inherit_id" ref="web.webclient_bootstrap"/>
    <field name="arch" type="xml">
        <xpath expr="/*" position="after"> 
            <div class="custom-navbar-logo">
                <img src="/crm_custom_fields/static/src/img/logoblanco_esparta.png" alt="Logo Personalizado"/>
            </div>
        </xpath>
    </field>
</record>